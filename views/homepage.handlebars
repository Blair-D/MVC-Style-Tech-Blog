<main class="container">
  <h1 >Tech Blog Posts</h1>
  <div class="row">
    {{#each posts}}
      <div >
        <div >
          <div>
            <h2><a href="/post/{{id}}">{{title}}</a></h2> 
          </div>
          <div>
            <p> date: {{format_date createdAt}} | created by: {{user.username}}</p>
          </div>
        </div>
      </div>
    {{/each}}
  </div>
</main>